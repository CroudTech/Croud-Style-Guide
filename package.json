  "private": false,
  "scripts": {
    "semantic:build": "gulp --cwd semantic/ build",
    "styleguide:watch": "gulp --cwd src/resources/",
    "styleguide:build": "cd src/resources/build/ && node build",
    "tech": "vue-styleguidist server",
    "tech:build": "vue-styleguidist build",
    "dev:styleguide": "yarn run styleguide:watch | node build/dev-server.js",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js && node checkUpdates",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "stylelint:staged": "node node_modules/stylelint/bin/stylelint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.vue": "stylelint:staged"
  },
